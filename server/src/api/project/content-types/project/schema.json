{
	"kind": "collectionType",
	"collectionName": "projects",
	"info": {
		"singularName": "project",
		"pluralName": "projects",
		"displayName": "Project"
	},
	"options": {
		"draftAndPublish": true
	},
	"pluginOptions": {},
	"attributes": {
		"title": {
			"type": "string",
			"required": true,
			"unique": true
		},
		"slug": {
			"type": "uid",
			"targetField": "title"
		},
		"abstract": {
			"type": "text"
		},
		"region": {
			"type": "enumeration",
			"enum": [
				"local",
				"regional",
				"national",
				"international",
				"other"
			],
			"default": "national"
		},
		"themes": {
			"type": "json"
		},
		"partners": {
			"type": "json"
		},
		"doc_url": {
			"type": "string"
		},
		"official_url": {
			"type": "string"
		},
		"lead": {
			"type": "relation",
			"relation": "manyToOne",
			"target": "api::person.person",
			"inversedBy": "leading_projects"
		},
		"members": {
			"type": "relation",
			"relation": "manyToMany",
			"target": "api::person.person",
			"inversedBy": "projects"
		},
		"domains": {
			"type": "relation",
			"relation": "manyToMany",
			"target": "api::department.department",
			"inversedBy": "projects"
		},
		"publications": {
			"type": "relation",
			"relation": "manyToMany",
			"target": "api::publication.publication",
			"mappedBy": "projects"
		}
	}
}
